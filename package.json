{
  "name": "kysely-bun-psql",
  "version": "0.0.1",
  "description": "Kysely dialect for PostgreSQL using the Postgres.js client",
  "repository": "https://github.com/nvsd/kysely-bun-psql",
  "homepage": "https://github.com/nvsd/kysely-bun-psql",
  "author": "Zack Novosad <zachary.novosad@gmail.com>",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "bun": "./dist/index.js",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "kysely",
    "bun",
    "psql",
    "postgresql",
    "dialect"
  ],
  "scripts": {
    "build": "bun --bun run build.config.ts",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "lint": "eslint src",
    "prepack": "bun --bun run lint && bun --bun build",
    "test": "bun --bun test --preload ./src/tests/setup.ts",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "kysely": ">= 0.24.0 < 1"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "bun-types": "latest",
    "eslint": "^9.19.0",
    "kysely": "^0.27.5",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  },
  "sideEffects": false
}
